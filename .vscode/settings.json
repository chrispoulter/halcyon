{
    "files.exclude": {
        "**/node_modules": true,
        "**/.next": true,
        "**/.swc": true,
        "**/*.tsbuildinfo": true,
        "**/playwright": true,
        "**/playwright-report": true,
        "**/test-results": true
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "jest.runMode": "on-demand"
}
