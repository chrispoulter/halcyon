{
    "name": "@halcyon/halcyon",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "prisma:generate": "prisma generate",
        "prisma:migrate:deploy": "prisma migrate deploy",
        "docker:build": "docker build . -t chrispoulter/halcyon",
        "docker:run": "docker run --name halcyon --env-file .env -p 3000:3000 chrispoulter/halcyon",
        "docker:stop": "docker container rm halcyon --force",
        "docker:exec": "docker exec -it halcyon /bin/sh"
    },
    "dependencies": {
        "@prisma/client": "4.12.0",
        "@reduxjs/toolkit": "1.9.3",
        "bcryptjs": "2.4.3",
        "bootstrap": "5.2.3",
        "bootswatch": "5.2.3",
        "formik": "2.2.9",
        "jsonwebtoken": "9.0.0",
        "jwt-decode": "3.1.2",
        "next": "13.3.0",
        "next-connect": "0.13.0",
        "next-redux-wrapper": "8.1.0",
        "nodemailer": "6.9.1",
        "pg": "8.10.0",
        "pino-http": "8.3.3",
        "pino-pretty": "10.0.0",
        "react": "18.2.0",
        "react-bootstrap": "2.7.4",
        "react-dom": "18.2.0",
        "react-redux": "8.0.5",
        "sass": "1.62.0",
        "uuid": "9.0.0",
        "yup": "1.1.1"
    },
    "devDependencies": {
        "eslint": "8.38.0",
        "eslint-config-next": "13.3.0",
        "eslint-config-prettier": "8.8.0",
        "prettier": "2.8.7",
        "prisma": "4.12.0"
    }
}
