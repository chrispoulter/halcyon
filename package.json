{
    "name": "@halcyon/halcyon",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"yarn:serverless:start\" \"yarn:client:start\"",
        "client:start": "react-scripts start",
        "client:build": "react-scripts build",
        "postclient:build": "yarn client:workbox",
        "client:workbox": "workbox injectManifest workbox-config.js",
        "serverless:start": "serverless offline start",
        "serverless:build": "serverless client build --command client:build",
        "serverless:deploy": "serverless deploy",
        "serverless:remove": "serverless remove",
        "fauna": "babel-node scripts/fauna.js",
        "seed": "babel-node scripts/seed.js",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\""
    },
    "dependencies": {
        "@apollo/react-hooks": "4.0.0",
        "@sentry/react": "6.2.1",
        "@sentry/serverless": "6.2.1",
        "@sentry/tracing": "6.2.1",
        "apollo-boost": "0.4.9",
        "apollo-server": "2.21.0",
        "apollo-server-lambda": "2.21.0",
        "bcryptjs": "2.4.3",
        "bootstrap": "4.6.0",
        "bootswatch": "4.6.0",
        "faunadb": "4.1.1",
        "formik": "2.2.6",
        "graphql": "15.5.0",
        "graphql-iso-date": "3.6.1",
        "graphql-resolvers": "0.4.2",
        "i18next": "19.9.1",
        "i18next-browser-languagedetector": "6.0.1",
        "i18next-http-backend": "1.1.1",
        "jsonwebtoken": "8.5.1",
        "jwt-decode": "3.1.2",
        "node-fetch": "2.6.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet": "6.1.0",
        "react-i18next": "11.8.8",
        "react-router-dom": "5.2.0",
        "react-toastify": "7.0.3",
        "reactstrap": "8.9.0",
        "reactstrap-confirm": "1.2.0",
        "uuid": "8.3.2",
        "yup": "0.32.9"
    },
    "devDependencies": {
        "@babel/core": "7.13.8",
        "@babel/node": "7.13.0",
        "@babel/plugin-proposal-class-properties": "7.13.0",
        "@babel/preset-env": "7.13.9",
        "babel-loader": "8.1.0",
        "babel-plugin-inline-import": "3.0.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.7.0",
        "concurrently": "6.0.0",
        "dotenv": "8.2.0",
        "node-sass": "5.0.0",
        "prettier": "2.2.1",
        "react-scripts": "4.0.3",
        "serverless": "2.28.7",
        "serverless-build-client": "2.5.0",
        "serverless-offline": "6.8.0",
        "serverless-offline-sns": "0.68.0",
        "serverless-pseudo-parameters": "2.5.0",
        "serverless-s3-sync": "1.15.0",
        "serverless-webpack": "5.3.5",
        "webpack": "4.44.2",
        "workbox-cli": "6.1.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
