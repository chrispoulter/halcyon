{
    "name": "halcyon-nextjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
        "start": "next start",
        "lint": "next lint",
        "db:seed": "babel-node scripts/seed.js",
        "db:populate": "babel-node scripts/populate.js",
        "db:build": "babel scripts --out-dir dist/scripts --source-maps --copy-files",
        "docker:build": "docker build . -t chrispoulter/halcyon-node",
        "docker:run": "docker run --name halcyon-node --env-file .env -p 3001:3001 chrispoulter/halcyon-node",
        "docker:exec": "docker exec -it halcyon-node /bin/sh",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\""
    },
    "dependencies": {
        "bcryptjs": "2.4.3",
        "bootstrap": "5.1.3",
        "bootswatch": "5.1.3",
        "formik": "2.2.9",
        "jsonwebtoken": "8.5.1",
        "jwt-decode": "3.1.2",
        "next": "12.1.6",
        "next-connect": "0.12.2",
        "nodemailer": "6.7.5",
        "pg": "8.7.3",
        "postgres-migrations": "5.3.0",
        "react": "18.1.0",
        "react-bootstrap": "2.4.0",
        "react-dom": "18.1.0",
        "sass": "1.52.1",
        "uuid": "8.3.2",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "eslint": "8.16.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "8.5.0",
        "prettier": "2.6.2"
    }
}
