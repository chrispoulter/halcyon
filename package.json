    "devDependencies": {
        "@babel/core": "7.11.1",
        "@babel/preset-env": "7.11.0",
        "babel-loader": "8.1.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "6.23.0",
        "babel-preset-env": "1.6.0",
        "concurrently": "5.3.0",
        "node-sass": "4.14.1",
        "prettier": "2.2.1",
        "react-scripts": "4.0.1",
        "serverless": "2.16.0",
        "serverless-build-client": "2.5.0",
        "serverless-dotenv-plugin": "3.1.0",
