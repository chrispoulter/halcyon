{
    "name": "@halcyon/halcyon",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "export": "next export",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\"",
        "prisma:generate": "prisma generate",
        "prisma:migrate:deploy": "prisma migrate deploy",
        "docker:build": "docker build . -t chrispoulter/halcyon",
