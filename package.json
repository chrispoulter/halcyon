{
    "name": "@halcyon/halcyon",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"yarn:serverless:start\" \"yarn:client:start\"",
        "client:start": "react-scripts start",
        "client:build": "react-scripts build",
        "postclient:build": "yarn client:workbox",
        "client:workbox": "workbox injectManifest workbox-config.js",
        "serverless:start": "serverless offline start",
        "serverless:build": "serverless client build --command client:build",
        "serverless:deploy": "serverless deploy",
        "serverless:remove": "serverless remove",
        "db:install": "serverless dynamodb install",
        "db:start": "serverless dynamodb start",
        "db:seed": "babel-node scripts/seed.js",
        "db:populate": "babel-node scripts/populate.js",
        "db:remove": "rimraf ./.dynamodb/shared-local-instance.db",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\""
    },
    "dependencies": {
        "@apollo/react-hooks": "4.0.0",
        "@sentry/react": "6.3.5",
        "@sentry/serverless": "6.3.5",
        "@sentry/tracing": "6.3.5",
        "apollo-boost": "0.4.9",
        "apollo-server": "2.23.0",
        "apollo-server-lambda": "2.23.0",
        "bcryptjs": "2.4.3",
        "bootstrap": "4.6.0",
        "bootswatch": "4.6.0",
        "formik": "2.2.6",
        "graphql": "15.5.0",
        "graphql-iso-date": "3.6.1",
        "graphql-resolvers": "0.4.2",
        "i18next": "20.2.2",
        "i18next-browser-languagedetector": "6.1.0",
        "i18next-http-backend": "1.2.2",
        "jsonwebtoken": "8.5.1",
        "jwt-decode": "3.1.2",
        "node-fetch": "2.6.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-i18next": "11.8.15",
        "react-router-dom": "5.2.0",
        "react-toastify": "7.0.4",
        "reactstrap": "8.9.0",
        "reactstrap-confirm": "1.2.0",
        "uuid": "8.3.2",
        "yup": "0.32.9"
    },
    "devDependencies": {
        "@babel/core": "7.14.0",
        "@babel/node": "7.13.13",
        "@babel/plugin-proposal-class-properties": "7.13.0",
        "@babel/preset-env": "7.14.0",
        "babel-loader": "8.1.0",
        "babel-plugin-inline-import": "3.0.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.7.0",
        "concurrently": "6.0.2",
        "dotenv": "8.2.0",
        "faker": "5.5.3",
        "node-sass": "5.0.0",
        "prettier": "2.2.1",
        "react-scripts": "4.0.3",
        "serverless": "2.39.0",
        "serverless-build-client": "2.5.0",
        "serverless-dynamodb-local": "0.2.39",
        "serverless-offline": "7.0.0",
        "serverless-offline-sns": "0.68.0",
        "serverless-pseudo-parameters": "2.5.0",
        "serverless-s3-sync": "1.17.1",
        "serverless-webpack": "5.4.2",
        "webpack": "4.44.2",
        "workbox-cli": "6.1.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
