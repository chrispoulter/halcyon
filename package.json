{
    "name": "@halcyon/halcyon",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"yarn:start:*\"",
        "start:api": "serverless offline start",
        "start:client": "react-scripts start",
        "build": "react-scripts build",
        "postbuild": "yarn workbox",
        "predeploy": "serverless client build",
        "deploy": "serverless deploy",
        "reset": "serverless remove",
        "workbox": "workbox injectManifest workbox-config.js",
        "fauna": "node scripts/fauna.js",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "format": "prettier --write \"**/*.{js,jsx,json,scss,md,yml,html}\""
    },
    "dependencies": {
        "@apollo/react-hooks": "4.0.0",
        "apollo-boost": "0.4.9",
        "apollo-server": "2.19.0",
        "apollo-server-lambda": "2.19.0",
        "bcryptjs": "2.4.3",
        "bootstrap": "4.5.3",
        "bootswatch": "4.5.3",
        "dotenv": "8.2.0",
        "faunadb": "4.0.0",
        "formik": "2.2.6",
        "graphql": "15.4.0",
        "graphql-iso-date": "3.6.1",
        "graphql-resolvers": "0.4.2",
        "i18next": "19.8.4",
        "i18next-browser-languagedetector": "6.0.1",
        "i18next-http-backend": "1.1.0",
        "jsonwebtoken": "8.5.1",
        "jwt-decode": "3.1.2",
        "node-fetch": "2.6.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet": "6.1.0",
        "react-i18next": "11.8.5",
        "react-router-dom": "5.2.0",
        "react-toastify": "6.2.0",
        "reactstrap": "8.7.1",
        "reactstrap-confirm": "1.2.0",
        "uuid": "8.3.2",
        "yup": "0.32.8"
